.popup-appointment {
	position: fixed;
	width: 100vw;
	height: 100vh;
	z-index: 99999999;
	overflow: hidden;
	right: 0;
	top: 0;
	background: #000000de;
	display: none;
}
.popup-appointwrap{
	overflow: auto;
	display: block;
	width: 60%;
	height: auto;
	/* margin: 30px auto; */
	position: relative;
	padding: 40px;
	background-color: white;
	top: 50%;
	left: 50%;
	margin-right: -50%;
	transform: translate(-50%, -50%);

}
.popup-appointment .book-cross {
	display: block;
	font-size: 25px;
	position: absolute;
	top: 10px;
	z-index: 9;
	cursor: pointer;
	top: 6%;
	left: 94.5%;
	margin-right: -50%;
	transform: translate(-50%, -50%);
}
.popup-appointment .book-cross i{
	height: 30px;
	width: 30px;
	border-radius: 50%;
	background-position: revert;
	position: relative;
	box-shadow: 0 3px 10px rgb(0 0 0 / 20%);
}
.popup-appointment .book-cross i:before{
	position: absolute;
	top: 50%;
	left: 50%;
	-ms-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);
	width: 100%;
	text-align: center;
	font-size: 16px;
}
.popup-appointment input, textarea{
	border: 1px solid #000000;
	margin-bottom: 12px;
	background: transparent;
	box-shadow: none;
	/* font-family: 'neuemontreal'; */
	font-family: 'open-sans-medium';
	letter-spacing: 2px;
	padding: 1.6rem;
	font-size: 14px;
	color: #000000;
}
.popup-appointment textarea{
	height:100px;
}
.popup-appointment input[type="submit"]{
	background-color: #d0d1cf!important;
	margin-top: 25px;
	width: 150px;
	padding: 12px!important;
	/* border-radius: 20px; */
	font-family: 'open-sans-medium'!important;
	/* font-weight: 600; */
	font-size: 18px;
	border: none!important;
	color: #000000;
	margin-left:60px;
}
.tempus-textarea{
	position: relative;
	display: block;
	overflow: hidden;
	width: 100%;
	padding-right: 8px;
}
.tempus-textarea textarea::placeholder{
	color:#000000;
}
.tempus-textarea textarea{
	border: 1px solid #000000;
}
.tempus-book ::-webkit-input-placeholder { /* WebKit browsers */
	color: #000 !important;
	opacity: 1;
}
.tempus-book textarea::placeholder{
	color: #000000;
	opacity: 1;
}
.tempus-book{
	/* margin-right: 51px; */
	font-family: Myriad Pro;
	width: 50%;
	padding-right: 8px;
	float: left;

}
.book-cross i{
	color: #000000;
}
.heading-contact-book h2{
	margin-bottom: 20px;
}
